<div class="songTime">
  <time class="currentTime">0:00</time>
  <div class="scrubber">
    <div class="handle"></div>
    <div class="timeline">
      <div class="loadingProgress"></div>
      <%= render :partial => "marker", :collection => song.section_markers, :locals => {:song => song} %>
    </div>
  </div>
  <time class="totalTime"><%= ms_to_MMSS song.duration %></time>
</div>