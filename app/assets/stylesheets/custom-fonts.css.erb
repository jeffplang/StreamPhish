@font-face {
  font-family: 'Economica';
  src: url('<%= font_path "Economica-Bold.eot" %>'); /* IE9 Compat Modes */
  src: url('<%= font_path "Economica-Bold.eot?#iefix" %>') format('embedded-opentype'), /* IE6-IE8 */
       url('<%= font_path "Economica-Bold.woff" %>') format('woff'), /* Modern Browsers */
       url('<%= font_path "Economica-Bold.ttf" %>')  format('truetype'), /* Safari, Android, iOS */
       url('<%= font_path "Economica-Bold.svg#d9532a473398f70deb182a9f6b5ed782" %>') format('svg'); /* Legacy iOS */
       
  font-style:   normal;
  font-weight:  700;
}

@font-face {
  font-family: 'Economica';
  src: url('<%= font_path "Economica-Regular.eot" %>'); /* IE9 Compat Modes */
  src: url('<%= font_path "Economica-Regular.eot?#iefix" %>') format('embedded-opentype'), /* IE6-IE8 */
       url('<%= font_path "Economica-Regular.woff" %>') format('woff'), /* Modern Browsers */
       url('<%= font_path "Economica-Regular.ttf" %>')  format('truetype'), /* Safari, Android, iOS */
       url('<%= font_path "Economica-Regular.svg#1b8abc1602823d353989443dbeda359c" %>') format('svg'); /* Legacy iOS */
       
  font-style:   normal;
  font-weight:  400;
}

