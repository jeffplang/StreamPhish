<h1 class="pageListHeader">
  <%= title %>
</h1>
<a href="/songs" class="backBtn">‚Üê Songs</a>

<ul class="pageList songs">
  <% _.each(tracks, function(track) { %>
    <li>
      <div class="songInfo">
        <span class="songDate">
          <a href="/shows/<%= track.show.show_date %>"><%= track.show.show_date %></a>
        </span>
        <br>
        <span class="songVenue"><%= track.show.location %></span>
      </div>
    </li>
  <% }); %>
</ul>