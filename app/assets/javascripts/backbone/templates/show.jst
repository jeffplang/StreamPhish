<h1 class="pageListHeader"><%= Streamphish.Helpers.dateString(show_date, '%m-%d-%Y') %> - <%= location %></h1>
<a href="/shows?year=<%= year %>" class="backBtn">← <%= year %></a>

<ul class="pageList songs">
  <% _.each(tracks, function(track) { %>
    <li>
      <div class="songInfo">
        <div class="songTitle">
          <a href="#"><%= track.position %> <%= track.title %></a>
        </div>
      </div>
    </li>
  <% }); %>
</ul>